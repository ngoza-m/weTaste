<%= render "shared/search" %>

<!-- <h1>Restaurants</h1> -->

<div id="restaurants">
  <% @restaurants.each do |restaurant| %>
    <p><%= restaurant.name %> </p>
    <p><%= restaurant.url %></p>
    <p><%= restaurant.location %></p>
    <p><%= link_to "Show this restaurant", restaurant %></p>
    
  <% end %>
</div>

<%= link_to "New restaurant", new_restaurant_path %>
